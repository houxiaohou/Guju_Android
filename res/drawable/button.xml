<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:constantSize="true">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#7e5e42" android:endColor="#7e5e42"
                android:angle="90.0">
            </gradient>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#9e7b5d" android:endColor="#9e7b5d"
                android:angle="90.0">
            </gradient>
        </shape>
    </item>
</selector>